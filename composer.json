{
    "name": "kassko/util-reflection",
    "description": "Add new capabilities to php native reflection",
    "license": "MIT",
    "keywords": ["reflection", "reflector", "doccomment", "docblock"],
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*",
        "phpunit/phpunit-mock-objects": "2.3.0",
        "kassko/util-member-accessor": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Kassko\\Util\\Reflection\\": "lib"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
